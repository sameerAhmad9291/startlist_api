{
  "version": 2,
  "installCommand": "yarn install",
  "buildCommand": "yarn run build",
  "outputDirectory": "dist",
  "devCommand": "yarn run start",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ]
}

{
  "version": 2,
  "installCommand": "yarn install",
  "buildCommand": "yarn run build",
  "outputDirectory": "dist",
  "devCommand": "yarn run start",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ]
}
